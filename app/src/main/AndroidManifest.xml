<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ainshafiqah.mysimplify">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.MySimplify">
        <activity android:name=".theorderActivity"></activity>
        <activity android:name=".OrderShipActivity" />
        <activity android:name=".OrderPackActivity" />
        <activity android:name=".OrderDeliverActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".CompletedOrderActivity" />
        <activity android:name=".AddOrderActivity" />
        <activity android:name=".UpdateStatusActivity" />
        <activity android:name=".StatusActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MoreActivity" />
        <activity android:name=".OrderActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>